{
  "name": "resume",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "generate:json": "yaml2json -p -s resume.yml",
    "export:html": "resume export index.html",
    "export:md": "resume export README.html -t md && npm run rename:readme",
    "export:pdf": "resume export resume.pdf",
    "pre-commit": "npm run generate:json && npm run export:html && npm run export:md && git add -A",
    "rename:readme": "node -e \"require('fs').rename('README.html', 'README.md', function(err) { if (err) console.error(err); console.log('File successfully renamed!') })\"",
    "test": "npm run generate:json && resume test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justingorham/resume.git"
  },
  "keywords": [],
  "author": "Justin Gorham (https://justin.gorh.am)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/justingorham/resume/issues"
  },
  "homepage": "https://github.com/justingorham/resume#readme",
  "devDependencies": {
    "husky": "^4.2.5",
    "jsonresume-theme-md": "^1.1.1",
    "resume-cli": "^1.2.7",
    "yamljs": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  }
}
